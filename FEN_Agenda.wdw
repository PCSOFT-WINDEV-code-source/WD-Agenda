     code : |1-
      // Résumé : Exécutée lors de la sélection d'une option de menu pour le classement
      PROCEDURE SurClassementEvénement(local nIDCatégorie est un entier)
      
      	// Si un rendez-vous a été sélectionné
      	si MonRendezVousSélectionné <> null alors
      
      		// Recherche l'événement
      		HLitRecherchePremier(AgendaEvénement, IDAgendaEvénement, MonRendezVousSélectionné..ID)
      		si htrouve(AgendaEvénement) ALORS
      			
      			si nIDCatégorie > 0 alors
      			
      				// Recherche la catégorie attribuée
      				HLitRecherchePremier(AgendaCatégorie, IDAgendaCatégorie, nIDCatégorie)
      				SI HTrouve(AgendaCatégorie) ALORS
      			
      					// Attribue la catégorie
      					AgendaEvénement.IDCatégorie = nIDCatégorie
      
      					// Applique la catégorie et la couleur de la catégorie
      					MonRendezVousSélectionné..Catégorie 	= AgendaCatégorie.Libellé
      					MonRendezVousSélectionné..CouleurFond	= AgendaCatégorie.Couleur = -1 ? RVB(191, 210, 234) sinon AgendaCatégorie.Couleur	
      
      				fin
      				
      			sinon
      
      				// Annule l'association
      				AgendaEvénement.IDCatégorie = 0
      				
      				// Applique la catégorie et la couleur de la catégorie par défaut
      				MonRendezVousSélectionné..Catégorie 	= ""
      				MonRendezVousSélectionné..CouleurFond	= RVB(191, 210, 234)
      			fin
      			
      			// Enregistre l'association
      			hmodifie(AgendaEvénement)
      		FIN		
      	fin
