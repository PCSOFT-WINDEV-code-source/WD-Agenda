      // Résumé : Exécuté lors de la liaison d'un événement à un agenda externe
      PROCEDURE SurLiaisonEvénement(local nOrigine est un entier, local nIDAgenda est un entier)
      
      	// Si un rendez-vous a été sélectionné
      	si MonRendezVousSélectionné <> null alors
      
      		// Recherche le rendez-vous
      		HLitRecherchePremier(AgendaEvénement, IDAgendaEvénement, MonRendezVousSélectionné..ID)
      		si htrouve(AgendaEvénement) alors
      
      			// En fonction de l'agenda
      			selon nOrigine
      				
      				// Outlook
      				cas depuisOutlook
      					
      					// Annule la liaison
      					si nIDAgenda = -1 ALORS
      						
      						// Supprime l'événement dans l'agenda Outlook
      						OutlookSupprimeEvénement(AgendaEvénement.IDAgendaOutlook, AgendaEvénement.IdentifiantOutlook)
      						
      						AgendaEvénement.IDAgendaOutlook = 0
      						AgendaEvénement.IdentifiantOutlook = ""						
      					sinon
      						AgendaEvénement.IDAgendaOutlook = nIDAgenda
      					FIN
      					
      				// Notes
      				cas depuisNotes
      					
      					// Annule la liaison
      					SI nIDAgenda = -1 ALORS
      						
      						// Supprime l'événement dans l'agenda Lotus
      						NotesSupprimeEvénement(AgendaEvénement.IDAgendaNotes, AgendaEvénement.IdentifiantNotes)
      						
      						AgendaEvénement.IDAgendaNotes = 0
      						AgendaEvénement.IdentifiantNotes = ""
      					SINON
      						AgendaEvénement.IDAgendaNotes = nIDAgenda
      					FIN
      					
      				// Google
      				cas depuisGoogle
      					
      					// Annule la liaison
      					SI nIDAgenda = -1 ALORS
      						
      						// Supprime l'événement dans l'agenda Google
      						GoogleSupprimeEvénement(AgendaEvénement.IDAgendaGoogle, AgendaEvénement.IdentifiantGoogle)
      												
      						AgendaEvénement.IDAgendaGoogle = 0
      						AgendaEvénement.IdentifiantGoogle = ""
      					SINON
      						AgendaEvénement.IDAgendaGoogle = nIDAgenda
      					FIN
      			FIN
      			
      			// Enregistre les modifications
      			hmodifie(AgendaEvénement)
      		fin
      		
      		MonRendezVousSélectionné <- Null
      				
      		// Actualise les événements
      		AfficheEvénements()
      		
      		// Re*sélectionne le rendez-vous
      		AgendaSelectPlus(AGD_AGENDA, RendezVousIndice)
      	fin
     type : 458752
