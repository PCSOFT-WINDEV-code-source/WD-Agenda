      // Résumé : Exécuté lors de la suppression d'un événement
      PROCEDURE SurSuppressionEvénement(MonRendezVous est un RendezVous)
      
      	sListeAgendaExterne est une chaine
      
      	// Désactive temporairement les triggers
      	HDésactiveTrigger()
      
      	// Recherche l'événement dans la base
      	HLitRecherchePremier(AgendaEvénement, IDAgendaEvénement, MonRendezVous..ID)
      	dbgAssertion(HTrouve(AgendaEvénement))
      	
      	si htrouve(AgendaEvénement) ALORS
      		
      		// Si l'événement est lié à un agenda externe
      		si AgendaEvénement.IDAgendaGoogle <> 0 _ou_ AgendaEvénement.IDAgendaNotes <> 0 _ou_ AgendaEvénement.IDAgendaOutlook <> 0 alors
      			
      			// Si l'événement est lié à un agenda Google
      			si AgendaEvénement.IDAgendaGoogle <> 0 ALORS
      				
      				// Recherche l'agenda
      				HLitRecherchePremier(AgendaGoogle, IDAgendaGoogle, AgendaEvénement.IDAgendaGoogle)
      				si htrouve(AgendaGoogle) ALORS
      					
      					// Complète la liste des agendas externes
      					sListeAgendaExterne += [rc] + "- " + AgendaGoogle.Nom + " (Google)"
      				FIN
      			FIN
      			
      			// Si l'événement est lié à un agenda Outlook
      			SI AgendaEvénement.IDAgendaOutlook <> 0 ALORS
      				
      				// Recherche l'agenda
      				HLitRecherchePremier(AgendaOutlook, IDAgendaOutlook, AgendaEvénement.IDAgendaOutlook)
      				SI HTrouve(AgendaOutlook) ALORS
      					
      					// Complète la liste des agendas externes
      					sListeAgendaExterne += [RC] + "- " + AgendaOutlook.Nom + " (Outlook)"
      				FIN
      			FIN
      			
      			// Si l'événement est lié à un agenda Notes
      			SI AgendaEvénement.IDAgendaNotes <> 0 ALORS
      				
      				// Recherche l'agenda
      				HLitRecherchePremier(AgendaNotes, IDAgendaNotes, AgendaEvénement.IDAgendaNotes)
      				SI HTrouve(AgendaNotes) ALORS
      					
      					// Complète la liste des agendas externes
      					sListeAgendaExterne += [RC] + "- " + AgendaNotes.Nom + " (Lotus)"
      				FIN
      			FIN
      			
      			//1 : &Supprimer
      			//2 : &Ne pas supprimer
      			SELON Dialogue(<§?1B03C0D9005353F1§>, sListeAgendaExterne)
      				// &Ne pas supprimer
      				CAS 2
      					RepriseSaisie()			
      			FIN
      			
      			// Marque l'événement comme supprimé
      			AgendaEvénement.Supprimé = Vrai		
      			
      			// Enregistre les modifications
      			HModifie(AgendaEvénement)
      		sinon
      			
      			// On ne demande pas de confirmation
      			// On supprime directement
      			hsupprime(AgendaEvénement)
      		fin
      	FIN
      	
      	// Ré-active les triggers
      	HActiveTrigger()
      
      	// Plus de rendez-vous sélectionné
      	MonRendezVousSélectionné <- null
      	ModeEditionRendezVous = 0
      	
      	PériodeSélectionnée.DateDébut 	= ""
      	PériodeSélectionnée.DateFin 	= ""
     type : 458752
