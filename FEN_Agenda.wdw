     name : SurSuppressionEvénement
     internal_properties : CAAAAAgAAAA41T8gHW4ikPb1ilBCeCiJH9Lf2vqVfmFJ8vWAuqJJJpHLkOaU91dKsR8WJqJP1C4n+MDLLi4h3qFG2E7FGMSQ3VRIuE1buo+EwK6BeVHv+CaAvB4vkia5Z4aNsu2HukfQH9on8fUlWUdLDAzde3/DDSAd1vWkKnyao+xdRrTFr/6ma5RKHNWO0SPs52d1QJE1hep6ZOLHWPAkXcWC8f1a4T4bdMdtvRdASA9WIG5nejWZyOJbL98PofQVWxFRnZnMCapgNgzEKxx8J3BpIyadh3m5l5M=
