      	DateFin 	est une dateheure		// Fin
      FIN
      
      CONSTANT
      	RdVAjout	= 1			// On vient d'ajouter un rendez-vous
      	RdvEdition	= 2			// On vient de modifier un rendez-vous
      FIN
      
      MonRendezVousSélectionné est un RendezVous 
