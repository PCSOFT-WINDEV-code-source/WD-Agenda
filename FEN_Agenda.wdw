     procedure_id : 1948532632510226102
     type_code : 14
     code : |1-
      // Résumé : Synchronisation des événements avec les agenda externes
      PROCEDURE SynchroniseEvénements()
      
      	// Ouvre la fenêtre de progression
      	OuvreFille(FEN_InfoSynchro)
      	
      	// On désactive le GFI temporairement pour éviter les effets de scintillement
      	// avec la fenêtre de conflit
      	FenActiveGFI(FEN_Agenda, Faux)
      	FenActiveGFI(FEN_InfoSynchro, Faux)
      	
      	// Borne de la jauge
      	FEN_InfoSynchro.JAUGE_TRAITEMENT..BorneMax = 3
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(0, "Synchronisation des agendas...")
      
      	// On commence par synchroniser les suppressions
      	SynchroniseEvénementsSuppressions()
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(1, "Synchronisation des agendas...")
      	
      	// On synchronise ensuite les nouvelles liaisons
      	SynchroniseEvénémentsAjouts()	
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(2, "Synchronisation des agendas...")
      
      	// On synchronise enfin les modifications
      	SynchroniseEvénementsModifications()
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(3, "Terminé")
      	
      	Ferme(FEN_InfoSynchro)
      	
      	// Réactive le GFI
      	FenActiveGFI(FEN_Agenda, Vrai)
     type : 458752
   -
     name : SynchroniseEvénementsSuppressions
     internal_properties : 
     procedure_id : 1948540874561857230
     type_code : 14
     code : |1-
      // Résumé : Synchronise les suppressions
      PROCEDURE SynchroniseEvénementsSuppressions()
      
      	TableauDeTableau est une structure
      		Eléments est un tableau de chaînes
      	FIN
      	
      	taSuppressionsOutlook 	est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taSuppressionsNotes 	est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taSuppressionsGoogle	est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	UnTableauDeTableau		est un TableauDeTableau
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(0, "Synchronisation des agendas...", "Rassemblement des événements supprimés...")
      	
      	// Désactive temporairement les triggers
      	HDésactiveTrigger()
      	
      	// Pour chaque événement marqué 'Supprimé'
      	POUR TOUT AgendaEvénement AVEC AgendaEvénement.Supprimé = Vrai
      		
      		// Si l'événement est lié à un agenda Google
      		SI AgendaEvénement.IDAgendaGoogle <> 0 _ET_ AgendaEvénement.IdentifiantGoogle <> "" ALORS
      			
      			// Récupère la liste des événements à supprimer
      			SI taSuppressionsGoogle[AgendaEvénement.IDAgendaGoogle] = Null ALORS
      				VariableRAZ(UnTableauDeTableau)
      			SINON
      				UnTableauDeTableau = taSuppressionsGoogle[AgendaEvénement.IDAgendaGoogle]
      			FIN
      			
      			// Ajoute l'événement à la liste
      			Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IdentifiantGoogle)
      			
      			// Conserve la nouvelle liste
      			taSuppressionsGoogle[AgendaEvénement.IDAgendaGoogle] = UnTableauDeTableau			
      		fin
      			
      		// Efface l'association
      		AgendaEvénement.IDAgendaGoogle 		= 0
      		AgendaEvénement.IdentifiantGoogle 	= ""
      		
      		// Si l'événement est lié à un agenda Notes
      		SI AgendaEvénement.IDAgendaNotes <> 0 _ET_ AgendaEvénement.IdentifiantNotes <> "" ALORS
      			
      			// Récupère la liste des événements à supprimer
      			SI taSuppressionsNotes[AgendaEvénement.IDAgendaNotes] = Null ALORS
      				VariableRAZ(UnTableauDeTableau)
      			SINON
      				UnTableauDeTableau = taSuppressionsNotes[AgendaEvénement.IDAgendaNotes]
      			FIN
      			
      			// Ajoute l'événement à la liste
      			Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IdentifiantNotes)
      			
      			// Conserve la nouvelle liste
      			taSuppressionsNotes[AgendaEvénement.IDAgendaNotes] = UnTableauDeTableau		
      		fin	
      		
      		// Efface l'association
      		AgendaEvénement.IDAgendaNotes 		= 0
      		AgendaEvénement.IdentifiantNotes 	= ""	
      		
      		// Si l'événement est lié à un agenda Notes
      		SI AgendaEvénement.IDAgendaOutlook <> 0 _ET_ AgendaEvénement.IdentifiantOutlook <> "" ALORS
      			
      			// Récupère la liste des événements à supprimer
      			SI taSuppressionsOutlook[AgendaEvénement.IDAgendaOutlook] = Null ALORS
      				VariableRAZ(UnTableauDeTableau)
      			SINON
      				UnTableauDeTableau = taSuppressionsOutlook[AgendaEvénement.IDAgendaOutlook]
      			FIN
      			
      			// Ajoute l'événement à la liste
      			Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IdentifiantOutlook)
      			
      			// Conserve la nouvelle liste
      			taSuppressionsOutlook[AgendaEvénement.IDAgendaOutlook] = UnTableauDeTableau				
      		fin
      			
      		// Efface l'association
      		AgendaEvénement.IDAgendaOutlook 		= 0
      		AgendaEvénement.IdentifiantOutlook 		= ""		
      		
      		// Si aucune association
      		SI AgendaEvénement.IDAgendaOutlook = 0 _ET_ AgendaEvénement.IDAgendaNotes = 0 _ET_ AgendaEvénement.IDAgendaGoogle = 0 ALORS
      			HSupprime(AgendaEvénement)
      		SINON
      			
      			// Enregistre les modifications
      			HModifie(AgendaEvénement)
      			
      			// Efface l'historique de cet événement
      			POUR TOUT AgendaEvénementHistorique AVEC IDAgendaEvénement = AgendaEvénement.IDAgendaEvénement
      				HSupprime(AgendaEvénementHistorique)
      			FIN
      		FIN
      	FIN
      	
      	// Ré-active les triggers
      	HActiveTrigger()
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(0, "Synchronisation des agendas...", "Suppression des événements dans les agendas Outlook...")
      	
      	// Effectue les suppressions Outlook
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taSuppressionsOutlook
      		OutlookSupprimeEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(0, "Synchronisation des agendas...", "Suppression des événements dans les agendas Lotus...")
      	
      	// Effectue les suppressions Lotus
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taSuppressionsNotes
      		NotesSupprimeEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(0, "Synchronisation des agendas...", "Suppression des événements dans les agendas Google...")
      	
      	// Effectue les suppressions Google
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taSuppressionsGoogle
      		GoogleSupprimeEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
     type : 458752
   -
     name : SynchroniseEvénementsAjouts
