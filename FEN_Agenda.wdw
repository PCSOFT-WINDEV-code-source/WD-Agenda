      // Résumé : Exécutée lors de la sélection d'une catégorie
      PROCEDURE SurSélectionCatégorie()
      
      	// RAZ de la liste des catégories sélectionnées
      	ListeCatégories = ""
      
      	pour toute ligne de TABLE_CATEGORIES
      		si COL_Selection ALORS
      			ListeCatégories += [";"] + COL_IDAgendaCatégorie
      		FIN
      	FIN
      	
      	// Mise à jour des rendez-vous
      	AfficheEvénements()
