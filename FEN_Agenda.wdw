     procedure_id : 1946610278131497892
     type_code : 14
     group : 5
     code : |1-
      // Résumé : Exécuté lors de la fin d'édition d'un événement
      PROCEDURE SurFinEditionEvénement(MonRendezVous est un RendezVous)
      
      	// Si on est en ajout
      	si ModeEditionRendezVous = RdVAjout ALORS
      		
      		// On vérifie que le rendez-vous possède un titre
      		si MonRendezVous..Titre = "" ALORS
      			
      			// Recherche le rendez-vous
      			HLitRecherchePremier(AgendaEvénement, IDAgendaEvénement, MonRendezVous..ID)
      			dbgAssertion(HTrouve(AgendaEvénement))
      			
      			SI HTrouve(AgendaEvénement) ALORS
      				
      				// On le supprime
      				HSupprime(AgendaEvénement)
      			FIN
      			
      			// Efface dans l'agenda
      			AgendaSupprimeRendezVous(AGD_AGENDA, AGD_AGENDA)
      			MonRendezVousSélectionné <- null
      			retour
      		FIN		
      	FIN
      
      	// Recherche l'événement
      	HLitRecherchePremier(AgendaEvénement, IDAgendaEvénement, MonRendezVous..id)
      	dbgAssertion(HTrouve(AgendaEvénement))
      	
      	si htrouve(AgendaEvénement) ALORS
      		
      		// Conserve le nouveau titre
      		AgendaEvénement.Titre = MonRendezVous..Titre
      		
      		// Enregistre les modifications
      		hmodifie(AgendaEvénement)		
      	FIN
      	
      	// Conserve l'événement "sélectionné"
      	SurSélectionEvénement(MonRendezVous)
      	
      fin:
      	ModeEditionRendezVous = 0
     type : 458752
