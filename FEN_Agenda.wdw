     type_code : 14
     code : |1-
      // Résumé : Synchronise les nouveaux événements
      PROCEDURE SynchroniseEvénementsAjouts()
      
      	TableauDeTableau est une structure
      		Eléments est un tableau d'entiers
      	FIN
      	
      	taAjoutsOutlook 	est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taAjoutsNotes 		est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taAjoutsGoogle		est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	UnTableauDeTableau	est un TableauDeTableau
      	
      	// Désactive temporairement les triggers
      	HDésactiveTrigger()
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(1, "Synchronisation des agendas...", "Rassemblement des événements ajoutés...")
      	
      	// On recherche les événements liés à un agenda Google mais pour lesquels on ne possède pas encore d'identifiant Google
      	POUR TOUT AgendaEvénement AVEC "IDAgendaGoogle <> 0 ET IdentifiantGoogle = ''"
      		
      		// Récupère la liste des ajouts
      		SI taAjoutsGoogle[AgendaEvénement.IDAgendaGoogle] = Null ALORS
      			VariableRAZ(UnTableauDeTableau)
      		SINON
      			UnTableauDeTableau = taAjoutsGoogle[AgendaEvénement.IDAgendaGoogle]
      		FIN
      		
      		// Ajoute l'événement à la liste
      		Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      		
      		// Conserve la nouvelle liste
      		taAjoutsGoogle[AgendaEvénement.IDAgendaGoogle] = UnTableauDeTableau
      		
      		// Efface l'historique de cet événement
      		pour TOUT AgendaEvénementHistorique avec IDAgendaEvénement = AgendaEvénement.IDAgendaEvénement
      			hsupprime(AgendaEvénementHistorique)
      		FIN
      	FIN
      	
      	// On recherche les événements liés à un agenda Outlook mais pour lesquels on ne possède pas encore d'identifiant Outlook
      	POUR TOUT AgendaEvénement AVEC "IDAgendaOutlook <> 0 ET IdentifiantOutlook = ''"
      		
      		// Récupère la liste des ajouts
      		SI taAjoutsOutlook[AgendaEvénement.IDAgendaOutlook] = Null ALORS
      			VariableRAZ(UnTableauDeTableau)
      		SINON
      			UnTableauDeTableau = taAjoutsOutlook[AgendaEvénement.IDAgendaOutlook]
      		FIN
      		
      		// Ajoute l'événement à la liste
      		Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      		
      		// Conserve la nouvelle liste
      		taAjoutsOutlook[AgendaEvénement.IDAgendaOutlook] = UnTableauDeTableau
      		
      		// Efface l'historique de cet événement
      		POUR TOUT AgendaEvénementHistorique AVEC IDAgendaEvénement = AgendaEvénement.IDAgendaEvénement
      			HSupprime(AgendaEvénementHistorique)
      		FIN
      	FIN
      	
      	// On recherche les événements liés à un agenda Notes mais pour lesquels on ne possède pas encore d'identifiant Notes
      	POUR TOUT AgendaEvénement AVEC "IDAgendaNotes <> 0 ET IdentifiantNotes = ''"
      		
      		// Récupère la liste des ajouts
      		SI taAjoutsNotes[AgendaEvénement.IDAgendaNotes] = Null ALORS
      			VariableRAZ(UnTableauDeTableau)
      		SINON
      			UnTableauDeTableau = taAjoutsNotes[AgendaEvénement.IDAgendaNotes]
      		FIN
      		
      		// Ajoute l'événement à la liste
      		Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      		
      		// Conserve la nouvelle liste
      		taAjoutsNotes[AgendaEvénement.IDAgendaNotes] = UnTableauDeTableau
      		
      		// Efface l'historique de cet événement
      		POUR TOUT AgendaEvénementHistorique AVEC IDAgendaEvénement = AgendaEvénement.IDAgendaEvénement
      			HSupprime(AgendaEvénementHistorique)
      		FIN
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(1, "Synchronisation des agendas...", "Export des événements ajoutés vers les agendas Outlook..")
      	
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taAjoutsOutlook
      		OutlookAjouteEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(1, "Synchronisation des agendas...", "Export des événements ajoutés vers les agendas Lotus..")
      	
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taAjoutsNotes
      		NotesAjouteEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(1, "Synchronisation des agendas...", "Export des événements ajoutés vers les agendas Google..")
      	
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taAjoutsGoogle
      		GoogleAjouteEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)
      	FIN
