      // Résumé : Prépare le menu contextuel
      PROCEDURE PrépareMenuContextuel()
      
      	nIndice est un entier
      	sNomOpt	est une chaine
      
      	// Grise les options de presse-papiers
      	OPT_CollerRendezVous..Etat = (RendezVousPressePapier = null ? grise sinon actif)
      	OPT_CopierRendezVous..Etat = (MonRendezVousSélectionné = null ? Grisé sinon actif)
      				
      	// Crée les options de menu pour les catégories
      	pour TOUT AgendaCatégorie depuisfin
      		
      		nIndice ++
      		
      		// Crée le nom de l'option
      		sNomOpt = "OPT_CAT" + AgendaCatégorie.IDAgendaCatégorie
      		
      		// Si l'option existe déjà
      		si MenuExiste(OPT_ClasserCatégorie, sNomOpt) alors
      		
      			quand exception dans 
      				MenuSupprime(sNomOpt)
      			faire
      				//
      			FIN
      		fin
      			
      		// Crée l'option de menu
      		MenuInsèreOption(OPT_ClasserCatégorie, OPT_SéparateurCatégories, sNomOpt, AgendaCatégorie.Libellé, SurClassementEvénement, AgendaCatégorie.IDAgendaCatégorie)				
      		
      		// Modifie l'image de l'option
      		{OPT_ClasserCatégorie..nomcomplet + "." + sNomOpt, indChamp}..Image = CréeImageCatégorie(sNomOpt, AgendaCatégorie.Couleur)
      		
      		// On affiche uniquement 5 éléments dans le menu
      		si nIndice > 5 ALORS
      			sortir
      		FIN
      	FIN
      	
      	nIndice = 0
      	
      	// Crée les options de menu pour les agendas externes Outlook
      	pour TOUT AgendaOutlook 
      		
      		// Crée le nom de l'option
      		sNomOpt = "OPT_OUTLOOK_" + AgendaOutlook.IDAgendaOutlook
      		nIndice ++
      		
      		QUAND EXCEPTION DANS 
      			MenuSupprime(sNomOpt)
      		FAIRE
      			//
      		FIN
      		
      		// Crée l'option de menu		
      		MenuInsèreOption(OPT_LierOutlook, OPT_Outlook_Sep, sNomOpt, AgendaOutlook.Nom, SurLiaisonEvénementOutlook, AgendaOutlook.IDAgendaOutlook)	
      	FIN
      		
      	MENU_Agenda.OPT_Lier.OPT_LierOutlook.OPT_Outlook_Sep..Visible = (nIndice > 0)
      	nIndice = 0
      	
      	// Crée les options de menu pour les agendas externes Notes
      	POUR TOUT AgendaNotes 
      		
      		// Crée le nom de l'option
      		sNomOpt = "OPT_NOTES_" + AgendaNotes.IDAgendaNotes
      		nIndice ++
      		
      		QUAND EXCEPTION DANS 
      			MenuSupprime(sNomOpt)
      		FAIRE
      			//
      		FIN
      		
      		// Crée l'option de menu
      		MenuInsèreOption(OPT_LierLotus_Notes, OPT_LierLotus_Sep, sNomOpt, AgendaNotes.Nom, SurLiaisonEvénementNotes, AgendaNotes.IDAgendaNotes)	
      	FIN
      	
      	MENU_Agenda.OPT_Lier.OPT_LierLotus_Notes.OPT_LierLotus_Sep..Visible = (nIndice > 0)
      	nIndice = 0
      	
      	// Crée les options de menu pour les agendas externes Google
      	POUR TOUT AgendaGoogle
      		
      		// Crée le nom de l'option
      		sNomOpt = "OPT_GOOGLE_" + AgendaGoogle.idAgendaGoogle
      		nIndice ++
      		
      		QUAND EXCEPTION DANS 
      			MenuSupprime(sNomOpt)
      		FAIRE
      			//
      		FIN
      		
      		// Crée l'option de menu
      		MenuInsèreOption(OPT_LierGoogle, OPT_LierGoogle_Sep, sNomOpt, AgendaGoogle.Nom, SurLiaisonEvénementGoogle, AgendaGoogle.IDAgendaGoogle)	
      	FIN
      	
      	MENU_Agenda.OPT_Lier.OPT_LierGoogle.OPT_LierGoogle_Sep..Visible = (nIndice > 0)
