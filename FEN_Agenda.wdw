      // Résumé : Déclenché lors de la création d'un rendez-vous
      PROCEDURE SurAjoutEvénement(MonRendezVous est an RendezVous)
      
      	nCouleur	est entier
      
      	HRAZ(AgendaEvénement)
      	
      	AgendaEvénement.Auteur 		= MonRendezVous..Auteur
      	AgendaEvénement.Contenu 		= MonRendezVous..Contenu
      	AgendaEvénement.DateDébut		= MonRendezVous..DateDébut
      	AgendaEvénement.DateFin		= MonRendezVous..DateFin
      	AgendaEvénement.IDCatégorie 	= CatégorieImporte(MonRendezVous..Catégorie, *, nCouleur)
      	AgendaEvénement.Invités		= MonRendezVous..Invité
      	AgendaEvénement.Lieux			= MonRendezVous..Lieu
      	AgendaEvénement.Titre			= MonRendezVous..Titre
      	AgendaEvénement.JournéeEntière	= MonRendezVous..JournéeEntière
      	
      	HAjoute(AgendaEvénement)
      	MonRendezVous..ID 				= AgendaEvénement.IDAgendaEvénement
      	MonRendezVous..CouleurFond		= (nCouleur < 0 ? RVB(191, 210, 234) SINON nCouleur)
      	
      	// Conserve le rendez-vous "sélectionné"
      	ModeEditionRendezVous = RdVAjout
      	SurSélectionEvénement(MonRendezVous)
     type : 458752
