      // Résumé : <indiquez ici ce que fait la procédure>
      PROCEDURE SynchroniseEvénementsModifications()
      
      	TableauDeTableau est une structure
      		Eléments est un tableau d'entiers
      	FIN
      	
      	taModifsOutlook 	est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taModifsNotes 		est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	taModifsGoogle		est un tableau associatif (*, *, wlEntier) de TableauDeTableau 
      	UnTableauDeTableau	est un TableauDeTableau
      
      	// Messages
      	FEN_InfoSynchro.Traitement(2, "Synchronisation des agendas...", "Rassemblement des événements modifiés...")
      
      	pour TOUT AgendaEvénement 
      		
      		// On recherche un historique pour cet événement
      		HRecherchePremier(AgendaEvénementHistorique, IDAgendaEvénement, AgendaEvénement.IDAgendaEvénement)
      		si htrouve(AgendaEvénementHistorique) ALORS
      						
      			// Si on a trouvé un historique, on doit mettre à jour ces modifications
      			si AgendaEvénement.IDAgendaGoogle <> 0 _ou_ AgendaEvénement.IDAgendaNotes <> 0 _ou_ AgendaEvénement.IDAgendaOutlook <> 0 ALORS
      				
      				// L'événement est lié à un agenda Google
      				si AgendaEvénement.IDAgendaGoogle <> 0 ALORS
      					
      					// Récupère la liste des modifications
      					SI taModifsGoogle[AgendaEvénement.IDAgendaGoogle] = Null ALORS
      						VariableRAZ(UnTableauDeTableau)
      					SINON
      						UnTableauDeTableau = taModifsGoogle[AgendaEvénement.IDAgendaGoogle]
      					FIN
      					
      					// Ajoute l'événement à la liste
      					Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      					
      					// Conserve la nouvelle liste
      					taModifsGoogle[AgendaEvénement.IDAgendaGoogle] = UnTableauDeTableau
      				FIN
      				
      				// L'événement est lié à un agenda Outlook
      				SI AgendaEvénement.IDAgendaOutlook <> 0 ALORS
      					
      					// Récupère la liste des modifications
      					SI taModifsOutlook[AgendaEvénement.IDAgendaOutlook] = Null ALORS
      						VariableRAZ(UnTableauDeTableau)
      					SINON
      						UnTableauDeTableau = taModifsOutlook[AgendaEvénement.IDAgendaOutlook]
      					FIN
      					
      					// Ajoute l'événement à la liste
      					Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      					
      					// Conserve la nouvelle liste
      					taModifsOutlook[AgendaEvénement.IDAgendaOutlook] = UnTableauDeTableau
      				FIN
      				
      				// L'événement est lié à un agenda Notes
      				SI AgendaEvénement.IDAgendaNotes <> 0 ALORS
      					
      					// Récupère la liste des modifications
      					SI taModifsNotes[AgendaEvénement.IDAgendaNotes] = Null ALORS
      						VariableRAZ(UnTableauDeTableau)
      					SINON
      						UnTableauDeTableau = taModifsNotes[AgendaEvénement.IDAgendaNotes]
      					FIN
      					
      					// Ajoute l'événement à la liste
      					Ajoute(UnTableauDeTableau:Eléments, AgendaEvénement.IDAgendaEvénement)
      					
      					// Conserve la nouvelle liste
      					taModifsNotes[AgendaEvénement.IDAgendaNotes] = UnTableauDeTableau
      				FIN
      			FIN
      		FIN
      	FIN
      	
      	// Messages
      	FEN_InfoSynchro.Traitement(2, "Synchronisation des agendas...", "Modification des événements dans les agendas Outlook...")
      	
      	// Effectue les modifications Outlook
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taModifsOutlook
      		OutlookModifieEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
      
      	// Messages
      	FEN_InfoSynchro.Traitement(2, "Synchronisation des agendas...", "Modification des événements dans les agendas Lotus...")
      
      	// Effectue les modifications Notes
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taModifsNotes
      		NotesModifieEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)	
      	FIN
      
      	// Messages
      	FEN_InfoSynchro.Traitement(2, "Synchronisation des agendas...", "Modification des événements dans les agendas Google...")
      
      	// Effectue les modifications Google
      	POUR TOUT UnTableauDeTableau, nIDAgenda de taModifsGoogle
      		GoogleModifieEvénementMultiples(nIDAgenda, UnTableauDeTableau:Eléments)
      	FIN
      	
      	// Efface l'historique des événements
      	HSupprimeTout(AgendaEvénementHistorique)
